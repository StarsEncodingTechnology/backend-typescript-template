  "scripts": {
    "build": "tsc",
    "start": "node lib/src/index.js | pino-pretty",
    "start:dev": "ts-node-dev --transpile-only src/index.ts | pino-pretty",
    "test": "yarn test:unit && yarn test:functional",
    "test:unit": "jest --runInBand --detectOpenHandles",
    "test:functional": "jest --projects ./test --runInBand --detectOpenHandles",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts",
    "fix": "yarn lint:fix && yarn style:fix"
  }
}
